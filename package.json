{
  "name": "Daedalus",
  "version": "0.0.1",
  "description": "Empty Epsilon - Maintainence, Repair and Operations extension",
  "main": "main.js",
  "scripts": {
    "osc": "/Applications/open-stage-control.app/Contents/MacOS/open-stage-control --osc-port 57122 --send 127.0.0.1:57121 --read--only --load /Users/amira/IdeaProjects/Daedalus/dashboards/panels.json ",
    "build:osc": "node dashboards/make.js",
    "build:client": "tsc -p node_modules/@fugazi/webclient/scripts & tsc -p node_modules/@fugazi/webclient/modules/scripts & echo 'done'",
    "reset:client": "yarn upgrade @fugazi/connector && yarn upgrade @fugazi/webclient && yarn prepublish",
    "prepublish": "yarn build:client",
    "clean": "rimraf ./dist",
    "build": "tsc -d",
    "pretest": "npm run clean && npm run build",
    "reset": "rimraf node_modules dist && yarn install && yarn test",
    "test": "mocha --require test-setup.js --reporter list \"dist/test/**/*.spec.js\"",
    "start": "rimraf dist && yarn build && node ./main.js",
    "go": "ts-node ./main.js",
    "debug": "rimraf dist && yarn build && concurrently --kill-others --success first \"yarn build -- -w\" \"node --inspect-brk ./main.js\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CommaSword/Daedalus.git"
  },
  "keywords": [
    "empty epsilon",
    "SBS",
    "OSC"
  ],
  "author": "Amir Arad <greenshade@gmail.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/CommaSword/Daedalus/issues"
  },
  "homepage": "https://github.com/CommaSword/Daedalus#readme",
  "devDependencies": {
    "@fugazi/connector": "^1.0.16",
    "@fugazi/webclient": "^1.0.16",
    "@types/autobahn": "^0.9.37",
    "@types/chai": "~4.0.4",
    "@types/lodash": "4.14.55",
    "@types/mocha": "~2.2.41",
    "@types/node": "8.0.33",
    "@types/resolve": "^0.0.4",
    "@types/sinon": "~2.3.1",
    "@types/sinon-chai": "^2.7.29",
    "@types/socket.io-client": "1.4.29",
    "@types/string-template": "^1.0.2",
    "chai": "~4.1.2",
    "chai-subset": "1.5.0",
    "concurrently": "~3.5.0",
    "mocha": "3.2.0",
    "nyc": "~11.2.1",
    "rimraf": "~2.6.1",
    "sinon": "~2.3.5",
    "sinon-chai": "^2.14.0",
    "source-map-support": "~0.5.0",
    "ts-node": "^3.3.0",
    "tslib": "~1.7.1",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "axios": "0.15.3",
    "eventemitter3": "2.0.2",
    "front-matter": "^2.2.0",
    "jsonschema": "^1.1.1",
    "kissfs": "^0.3.0",
    "lodash": "4.17.4",
    "naming": "^1.0.6",
    "osc": "^2.2.0",
    "resolve": "^1.4.0",
    "rxjs": "^5.5.2",
    "socket.io-client": "1.7.3",
    "socketio-wildcard": "0.4.0",
    "string-template": "^1.0.0"
  }
}
