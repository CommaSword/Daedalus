{
  "name": "scs",
  "version": "0.0.1",
  "description": "Ship Control System - Empty Epsilon integration server",
  "main": "dist/src/server.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc -d",
    "pretest": "npm run clean && npm run build",
    "reset": "rimraf node_modules dist && yarn install && yarn test",
    "test": "mocha --reporter mocha-env-reporter dist/test/index",
    "start": "rimraf dist && yarn build && node ./main.js",
    "debug": "rimraf dist && yarn build && concurrently --kill-others --success first \"yarn build -- -w\" \"node --inspect-brk ./main.js\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CommaSword/scs.git"
  },
  "keywords": [
    "empty epsilon",
    "SBS"
  ],
  "author": "Amir Arad (amira@wix.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CommaSword/scs/issues"
  },
  "homepage": "https://github.com/CommaSword/scs#readme",
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/chai-as-promised": "0.0.29",
    "@types/lodash": "4.14.55",
    "@types/mocha": "2.2.39",
    "@types/node": "8.0.33",
    "@types/socket.io-client": "1.4.29",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-subset": "1.5.0",
    "concurrently": "~3.5.0",
    "mocha": "3.2.0",
    "mocha-env-reporter": "2.0.4",
    "rimraf": "2.6.0",
    "sinon": "2.0.0-pre.2",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "axios": "0.15.3",
    "eventemitter3": "2.0.2",
    "lodash": "4.17.4",
    "socket.io-client": "1.7.3",
    "socketio-wildcard": "0.4.0",
    "source-map-support": "0.4.11"
  }
}
